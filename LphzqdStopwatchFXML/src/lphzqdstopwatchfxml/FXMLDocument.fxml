<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="anchorpane" prefHeight="650.0" prefWidth="1234.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lphzqdstopwatchfxml.LphzqdStopwatchFXMLController">
   <children>
      <GridPane fx:id="gridpane" alignment="CENTER" layoutX="20.0" layoutY="14.0" prefHeight="650.0" prefWidth="1234.0">
         <children>
            <StackPane fx:id="stackpane" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="handImageView" fitHeight="200.0" fitWidth="200.0">
                     <image>
                        <Image url="@hand.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="dialImageView" fitHeight="245.0" fitWidth="270.0">
                     <image>
                        <Image url="@clockface.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
            <Label fx:id="time" alignment="CENTER" contentDisplay="CENTER" prefHeight="110.0" prefWidth="411.0" GridPane.columnIndex="2" />
            <Button fx:id="start" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handlestart" prefHeight="52.0" prefWidth="120.0" text="Start" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <Button fx:id="reset" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handlereset" prefHeight="54.0" prefWidth="109.0" text="Reset" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <Button fx:id="stop" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handlestop" prefHeight="57.0" prefWidth="112.0" text="Stop" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
   </children>
</AnchorPane>
